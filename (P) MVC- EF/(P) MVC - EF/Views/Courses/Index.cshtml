@model IEnumerable<_P__MVC___EF.Models.Course>

@{
    ViewBag.Title = "Index";
}

<h2>Courses</h2>
<img src="data:image/jpeg;base64," style="height:25%; width: 25%;" />
<br />

@foreach (var item in Model) {
<div class="CourseInfo">
    @Html.DisplayFor(modelItem => item.CourseName)

    @Html.DisplayFor(modelItem => item.TermCode)

    @Html.DisplayFor(modelItem => item.CourseNumber)


    @Html.ActionLink("Details", "Details", new { id = item.CourseID }) |

</div>
}
<br />
<div class="row"></div>


